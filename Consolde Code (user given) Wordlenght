static void Main(string[] args)
        {
            Console.Write("Enter a word: ");
            string word=Console.ReadLine();
            wordlenght(word);
            Console.ReadKey();

        }

        static void wordlenght(string word)
        {
            for (int i = 0; i < word.Length; i++)
            {
                Console.WriteLine(word);
            }
        }
